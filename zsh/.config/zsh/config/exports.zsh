#!/bin/zsh
export _JAVA_AWT_WM_NONREPARENTING=1

export LANG=en_US.UTF-8
export EDITOR='nvim'
export TERMINAL="wezterm"
export BROWSER="brave"
export GTK_THEME=Adwaita:dark
export GTEST_COLOR=1

export PATH="$HOME/.local/bin:$PATH"
